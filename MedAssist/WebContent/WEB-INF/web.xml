<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MedAssistOrig</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.medical.controller.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>signup</servlet-name>
    <servlet-class>com.medical.controller.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signup</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.medical.controller.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>submitcase</servlet-name>
    <servlet-class>com.medical.controller.SubmitCase</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>submitcase</servlet-name>
    <url-pattern>/submitcase</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>history</servlet-name>
    <servlet-class>com.medical.controller.History</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>history</servlet-name>
    <url-pattern>/history</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>scheduleAppointment</servlet-name>
    <servlet-class>com.medical.controller.ScheduleAppointment</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>scheduleAppointment</servlet-name>
    <url-pattern>/scheduleAppointment</url-pattern>
  </servlet-mapping>
</web-app>